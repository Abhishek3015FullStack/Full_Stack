@model List< AbhishekPractice.Models.Book>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        <table>
                <tr>
                        <th>id</th>
                        <th>names</th>
                        <th>Price</th>
                        <th>Produced</th>
                </tr>
            @foreach(var a in Model)
            { 
                <tr>
                        <td>@a.id</td>
                        <td>@a.names</td>
                        <td>@a.price</td>
                        <td>@a.produced</td>
                </tr>
            }
        </table>
    </div>
</body>
</html>
