@model List<ProductsList.Models.Category>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table>
            <tr>
                    <td>CategoryId</td>
                    <td>categoryName</td>
            </tr>
        @foreach(var item in Model)
        { 
            <tr>
                    <td>@item.CategoryID</td>
                    <td>@item.CategoryName</td>
            </tr>
        }
    </table>
</body>
</html>
